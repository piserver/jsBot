{
  "manifest_version": 2,

  "name": "jsBot",
  "description": "jsBot Bakk prg",
  "version": "1.0",
  "browser_action": {
     "default_icon": "/images/icons/icon128.png",
     "default_title": "Click to to start crawling"
  },
  "icons": {
    "16": "/images/icons/icon16.png",
    "48": "/images/icons/icon48.png",
    "128": "/images/icons/icon128.png"
  },

  "content_scripts":[
    {
      "matches":["*://*/"],
      "js":["./public/jsBotComp.js"]
    }
  ],

  "background": {
    "scripts": ["./src/chrome.init.test.js"],
    "persistent": false
  },

  "permissions": [ "webNavigation","contextMenus","tabs","*://*/" ]
}
